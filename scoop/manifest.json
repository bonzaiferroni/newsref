{
  "manifest_version": 2,
  "name": "Borderify",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "build/dist/js/productionExecutable/icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://kotlinlang.org/*"
      ],
      "js": [
        "build/dist/js/productionExecutable/scoop-main.js"
      ]
    }
  ]
}
